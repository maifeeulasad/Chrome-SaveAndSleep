{
	"name": "Save And Sleep",
	"version": "0.1",
	"description": "Feeling dizzzyyyyyyyyyy?? Save all tabs and just take a nap.I'm sure, we need it.",
	"background": {
		"persistent": false,
		"scripts": ["background.js","util.js"]
	},
"commands": {
    "Save And Sleep": {
		"suggested_key": { 
			"default": "Alt+Shift+M" 
		},
		"description": "Save all opened tabs for later use"
    },
	"Restore And Breath": {
		"suggested_key": { 
			"default": "Alt+Shift+Q" 
		},
		"description": "Restore last session"
    }

  },
  "permissions": [
    	"tabs",
		"storage"
		],
	"browser_action":{
		"default_title" : "Save And Sleep",
		"default_icon": "mua_icon.png",
		"default_popup": "popup.html"
	},
	"manifest_version": 2
}
